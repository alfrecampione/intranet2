          <!-- Add Head Carrier Modal -->
          <div class="modal fade" id="addHeadCarrier" tabindex="-1" aria-hidden="true">
            <div class="modal-dialog modal-lg modal-simple modal-edit-user">
              <div class="modal-content p-3 p-md-5">
                <div class="modal-body">
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                  <div class="text-center mb-4">
                    <h3 class="mb-2">Add Head Carrier</h3>
                    <p class="text-muted">The head carriers groups all the programs offered by the companies</p>
                  </div>
                  <form id="editRfForm" class="row g-3" onsubmit="return validateForm()" method="post"
                    action="/users/config/headcarrier/addHeadCarrier">
                    <div class="col-12 col-md-6">
                      <label for="name" class="form-label">Head Carrier</label>
                      <input class="form-control" type="text" id="name" name="name" required />
                    </div>
                    <div class="col-12 col-md-6">
                      <label for="select2Basic" class="form-label">Carrier or MGA</label>
                      <select id="select2Basic" class="select2 form-select form-select-lg input-group-text"
                        data-allow-clear="true" name="carrier_id">
                        <%carriers.forEach(element=> {%>
                          <option value="<%=element.entity_id%>">
                            <%=element.display_name%>
                          </option>
                          <%})%>
                      </select>
                    </div>
                    <div class="col-12 text-center">
                      <button type="submit" class="btn btn-primary me-sm-3 me-1">Submit</button>
                      <button type="reset" class="btn btn-label-secondary" data-bs-dismiss="modal" aria-label="Close">
                        Cancel
                      </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
          <!--/ Add Head Carrier Modal -->

          <!-- Add Carrier to Head Carrier Modal -->
          <div class="modal fade" id="addCarrier" tabindex="-1" aria-hidden="true">
            <div class="modal-dialog modal-lg modal-simple modal-edit-user">
              <div class="modal-content p-3 p-md-5">
                <div class="modal-body">
                  <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                  <div class="text-center mb-4">
                    <h3 class="mb-2">Add Company to Head Carrier</h3>
                    <p class="text-muted">The head carriers groups all the programs offered by the companies</p>
                  </div>
                  <form id="editRfForm" class="row g-3" onsubmit="return validateForm()" method="post"
                    action="/users/config/headcarrier/addHeadCarrier">
                    <div class="col-12 col-md-6">
                      <label for="name1" class="form-label">Head Carrier</label>
                      <input class="form-control" type="text" id="name1" name="name" readonly />
                    </div>
                    <div class="col-12 col-md-6">
                      <label for="select2Basic" class="form-label">Carrier or MGA</label>
                      <select id="select2Basic" class="select2 form-select form-select-lg input-group-text"
                        data-allow-clear="true" name="carrier_id">
                        <%carriers.forEach(element=> {%>
                          <option value="<%=element.entity_id%>">
                            <%=element.display_name%>
