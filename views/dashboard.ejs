                <a class="nav-link dropdown-toggle hide-arrow" href="javascript:void(0);" data-bs-toggle="dropdown">
                  <i class="ti ti-md"></i>
                </a>
                <ul class="dropdown-menu dropdown-menu-end dropdown-styles">
                  <li>
                    <a class="dropdown-item" href="javascript:void(0);" data-theme="light">
                      <span class="align-middle"><i class="ti ti-sun me-2"></i>Light</span>
                    </a>
                  </li>
                  <li>
                    <a class="dropdown-item" href="javascript:void(0);" data-theme="dark">
                      <span class="align-middle"><i class="ti ti-moon me-2"></i>Dark</span>
                    </a>
                  </li>
                  <li>
                    <a class="dropdown-item" href="javascript:void(0);" data-theme="system">
                      <span class="align-middle"><i class="ti ti-device-desktop me-2"></i>System</span>
                    </a>
                  </li>
                </ul>
              </li>

              <!-- Notification -->
              <li class="nav-item dropdown-notifications navbar-dropdown dropdown me-3 me-xl-1">
                <a class="nav-link dropdown-toggle hide-arrow" href="javascript:void(0);" data-bs-toggle="dropdown"
                  data-bs-auto-close="outside" aria-expanded="false">
                  <i class="ti ti-bell ti-md"></i>
                  <!--<span class="badge bg-danger rounded-pill badge-notifications">5</span>-->
                </a>

                <ul class="dropdown-menu dropdown-menu-end py-0">
                  <li class="dropdown-menu-header border-bottom">
                    <div class="dropdown-header d-flex align-items-center py-3">
                      <h5 class="text-body mb-0 me-auto">Notification</h5>
                      <a href="javascript:void(0)" class="dropdown-notifications-all text-body" data-bs-toggle="tooltip"
                        data-bs-placement="top" title="Mark all as read"><i class="ti ti-mail-opened fs-4"></i></a>
                    </div>
                  </li>
              </li>
              <li class="dropdown-menu-footer border-top">
                <a href="javascript:void(0);"
                  class="dropdown-item d-flex justify-content-center text-primary p-2 h-px-40 mb-1 align-items-center">
                  View all notifications
                </a>
              </li>
            </ul>
            </li>
            <!--/ Notification -->

            <!-- User -->
            <li class="nav-item navbar-dropdown dropdown-user dropdown">
              <a class="nav-link dropdown-toggle hide-arrow" href="javascript:void(0);" data-bs-toggle="dropdown">
                <div class="avatar avatar-online">
                  <img src="/img/avatars/users/<%=user.user_id%>.png" alt class="h-auto rounded-circle" />
                </div>
              </a>
              <ul class="dropdown-menu dropdown-menu-end">
                <li>
                  <a class="dropdown-item" href="pages-account-settings-account.html">
                    <div class="d-flex">
                      <div class="flex-shrink-0 me-3">
                        <div class="avatar avatar-online">
                          <img src="/img/avatars/users/<%=user.user_id%>.png" alt class="h-auto rounded-circle" />
                        </div>
                      </div>
                      <div class="flex-grow-1">
                        <span class="fw-medium d-block">
                          <%= user.display_name %>
                        </span>
                        <small class="text-muted">
                          <%=user.job_title%>
                        </small>
                      </div>
                    </div>
                  </a>
                </li>
                <li>
                  <div class="dropdown-divider"></div>
                </li>
                <li>
                  <a class="dropdown-item" href="/users/logout"> <!-- target="_blank" -->
                    <i class="ti ti-logout me-2 ti-sm"></i>
                    <span class="align-middle">Log Out</span>
                  </a>
                </li>
              </ul>
            </li>
            <!--/ User -->
            </ul>
          </div>

          <!-- Search Small Screens -->
          <div class="navbar-search-wrapper search-input-wrapper d-none">
            <input type="text" class="form-control search-input container-xxl border-0" placeholder="Search..."
              aria-label="Search..." />
            <i class="ti ti-x ti-sm search-toggler cursor-pointer"></i>
          </div>
        </nav>

        <!-- / Navbar -->

        <!-- Content wrapper -->
        <div class="content-wrapper">

          <!-- Content -->
          <div class="container-xxl flex-grow-1 container-p-y">
            <div class="row">
              <!-- Policies/Premium Corporate/Company -->
              <div class="row mt-4 mb-3">


                <!-- Company Today -->
                <div class="col-sm-6 col-lg-3 mb-2">
                  <p class="mb-0 text-primary">
                    <%=agency%> - Today
                  </p>
                  <div class="card card-border-shadow-primary">
                    <div class="card-body pt-1">
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-user-dollar ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ct_nb_prem %> / <%= ct_nb_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-repeat ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0 mt-0">
                          <%= ct_rn_prem %> / <%= ct_rn_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-writing ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ct_rw_prem %> / <%= ct_rw_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-moneybag ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ct_tot_prem %> / <%= ct_tot_pol %>
                        </h5>
                      </div>
                    </div>
                  </div>
                </div>
                <!--/ Company Today -->

                <!-- Company this month -->
                <div class="col-sm-6 col-lg-3 mb-2">
                  <p class="mb-0 text-primary">
                    <%=agency%> - This Month
                  </p>
                  <div class="card card-border-shadow-primary">
                    <div class="card-body pt-1">
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-user-dollar ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cm_nb_prem %> / <%= cm_nb_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-repeat ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0 mt-0">
                          <%= cm_rn_prem %> / <%= cm_rn_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-writing ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cm_rw_prem %> / <%= cm_rw_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-moneybag ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cm_tot_prem %> / <%= cm_tot_pol %>
                        </h5>
                      </div>
                    </div>
                  </div>
                </div>
                <!--/ Company this month -->

                <!-- Company this year -->
                <div class="col-sm-6 col-lg-3 mb-2">
                  <p class="mb-0 text-primary">
                    <%=agency%> - Year to Date
                  </p>
                  <div class="card card-border-shadow-primary">
                    <div class="card-body pt-1">
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-user-dollar ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cy_nb_prem %> / <%= cy_nb_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-repeat ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0 mt-0">
                          <%= cy_rn_prem %> / <%= cy_rn_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-writing ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cy_rw_prem %> / <%= cy_rw_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-moneybag ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= cy_tot_prem %> / <%= cy_tot_pol %>
                        </h5>
                      </div>
                    </div>
                  </div>
                </div>
                <!--/ Company this year -->

                <!-- Year TKG -->
                <div class="col-sm-6 col-lg-3 mb-2">
                  <p class="mb-0 text-primary">
                    <%=agency%> - Year TKG
                  </p>
                  <div class="card card-border-shadow-primary">
                    <div class="card-body pt-1">
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-user-dollar ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ck_nb_prem %> / <%= ck_nb_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-repeat ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0 mt-0">
                          <%= ck_rn_prem %> / <%= ck_rn_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-writing ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ck_rw_prem %> / <%= ck_rw_pol %>
                        </h5>
                      </div>
                      <div class="d-flex align-items-center">
                        <div class="avatar me-2">
                          <span class="avatar-initial rounded bg-label-primary"><i
                              class="ti ti-moneybag ti-sm"></i></span>
                        </div>
                        <h5 class="ms-1 mb-0">
                          <%= ck_tot_prem %> / <%= ck_tot_pol %>
                        </h5>
                      </div>
                    </div>
                  </div>
                </div>
                <!--/ Year TKG -->

                <!-- NB Premium last quarter -->
                <div class="col-xl-2 col-md-4 col-6 mb-4">
                  <div class="card">
                    <div class="card-header pb-0">
                      <h5 class="card-title mb-0">NB Premium</h5>
                      <small class="text-muted">Last Quarter</small>
                    </div>
                    <div id="salesLastYear"></div>
                    <div class="card-body pt-0">
                      <div class="d-flex justify-content-between align-items-center mt-3 gap-3">
                        <h4 id="salesLastQuarter" class="mb-0"></h4>
                        <small id="porClastQuarter" class="text-warning"></small>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- NB Policies last quarter -->
                <div class="col-xl-2 col-md-4 col-6 mb-4">
                  <div class="card">
                    <div class="card-header pb-0">
                      <h5 class="card-title mb-0">NB Policies</h5>
                      <small class="text-muted">Last Quarter</small>
                    </div>
                    <div id="salesLastQuarterPolicies"></div>
                    <div class="card-body pt-0">
                      <div class="d-flex justify-content-between align-items-center mt-3 gap-3">
                        <h4 id="policiesLastQuarter" class="mb-0"></h4>
                        <small id="polPorClastQuarter" class="text-warning"></small>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- NB Premium last Week -->
              <div class="col-xl-2 col-md-4 col-6 mb-4">
                <div class="card">
                  <div class="card-body">
                    <div class="badge p-2 bg-label-info mb-2 rounded"><i class="ti ti-currency-dollar ti-md"></i></div>
                    <h5 class="card-title mb-1 pt-2">NB Premium</h5>
                    <small class="text-muted">Last week</small>
                    <p id="lastWeekPremium" class="mb-2 mt-1"></p>
                    <div class="pt-1">
                      <span id="lastWeekPremiumPer" class="badge bg-label-secondary"></span>
                    </div>
                  </div>
                </div>
              </div>

              <!-- NB Policies last Week -->
              <div class="col-xl-2 col-md-4 col-6 mb-4">
                <div class="card">
                  <div class="card-body">
                    <div class="badge p-2 bg-label-danger mb-2 rounded">
                      <i class="ti ti-shield ti-md"></i>
                    </div>
                    <h5 class="card-title mb-1 pt-2">NB Policies</h5>
                    <small class="text-muted">Last week</small>
                    <p id="lastWeekPolicies" class="mb-2 mt-1"></p>
                    <div class="pt-1">
                      <span id="lastWeekPoliciesPer" class="badge bg-label-secondary"></span>
                    </div>
                  </div>
                </div>
              </div>
              
              <!-- Week Sales -->
              <div class="col-xl-4 col-md-8 mb-4">
                <div class="card">
                  <div class="card-body">
                    <div class="d-flex justify-content-between">
                      <div class="d-flex flex-column">
                        <div class="card-title mb-auto">
                          <h5 class="mb-1 text-nowrap">NB Production Growth</h5>
                          <small>Weekly Report</small>
                        </div>
                        <div class="chart-statistics">
                          <h3 id="productionGrowthWeekPrem" class="card-title mb-1"></h3>
                          <h5 id="productionGrowthWeekPol" class="card-title mb-1"></h5>
                        </div>
                      </div>
                      <div id="revenueGrowth"></div>
                    </div>
                  </div>
                </div>
              </div>
              
              <!-- CSR Ranking-->
              <div class="col-xl-4 col-md-6 mb-4">
                  <div class="card h-100">
                    <div class="card-header d-flex justify-content-between">
                      <div class="card-title mb-0">
                        <h5 class="m-0 me-2">CSR Ranking</h5>
                        <small class="text-muted"><%=agency%> New Business</small>
                      </div>
                      <div>
                        <h6 class="pt-2"><b><%= cm_nb_prem %> / <%= cm_nb_pol %></b></h6>
                      </div>
                    </div>
                    <div class="card-body">
                      <ul class="p-0 m-0">
                        <% csr_nb_ranking.forEach(element => { %>
                        <li class="d-flex align-items-center mb-4">
                          <div class="avatar flex-shrink-0 me-3">
                            <% if(element.avatar) {%>
                              <img src="/img/avatars/users/<%=element.avatar%>" alt class="h-auto rounded-circle" />
                            <%}else{%>
                             <%-element.output%>
                            <%}%>
                          </div>
                          <div class="d-flex w-100 align-items-center justify-content-between gap-2">
                            <div class="me-2">
                              <div class="d-flex align-items-center">
                                <h6 class="mb-0 me-1"><%= element.csr %></h6>
                              </div>
                              <small class="text-muted"><%= element.location %></small>
                            </div>
                            <div>
                              <h6 class="mb-0 me-1"><%= element.premium %> / <%= element.policies %></h6>
                            </div> 
                          </div>
                        </li>
                        <% }) %>
                      </ul>
                    </div>
                  </div>
              </div>

              <!-- Sales Estatistics-->
              <div class="col-12 col-xl-8 mb-4">
                <div class="card">
                  <div class="card-header d-flex justify-content-between">
                    <div class="card-title mb-0">
                      <h5 class="mb-0">Production Statistic</h5>
